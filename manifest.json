{
  "manifest_version": 2,

  "name": "QRnicez",
  "description": "This extension shows a qr code in a small box",
  "version": "1.0",

  "icons": { "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://chart.googleapis.com/"
  ]
}
